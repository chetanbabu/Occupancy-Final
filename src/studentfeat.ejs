        </tr>
      </thead>
  
  
      <tbody>
  
        <%if (records.length >0){ %>
        <% records.forEach(function(row){ %>
    
      <br/>
      <br/>
       <tr><center>
          <!-- <td><center><%= row.sem %></center></td><th></th>
          <td><center><%= row.year %></center></td><th></th>
          <td><center><%= row.section %></center></td><th></th> -->
         
